"species.about.ape" = "¡Paraíso de primates! Se balancea entre la hora del bocadillo y la siesta como el influencer original de la jungla!";
"species.about.ape_chef" = "¡Rey Kong de la Cocina! ¡Volteando panqueques y volviéndose loco!";
"species.about.betta" = "¡Pez luchador con actitud! Hace volteretas, actúa tsundere y sirve drama acuático!";
"species.about.cat" = "¡El compañero de escritorio definitivo! Se relaja, come golosinas y duerme como la verdadera realeza felina!";
"species.about.cat_black" = "¡Travieso misterioso de medianoche! Duerme todo el día, te juzga toda la noche!";
"species.about.cat_blue" = "¡Frío como el hielo, suave como la seda! Este felino azul trae vibras zen a tu pantalla!";
"species.about.cat_calico" = "¡Problema de tres colores! Come, duerme y sirve looks con perfección de patchwork!";
"species.about.cat_floppa" = "¡Energía Big Floppa! Se alista para el servicio, come como un soldado y exige respeto con esas orejas icónicas!";
"species.about.cat_gray" = "¡El clásico caballero gris! Comer sofisticado, siesta profesional y mirada de juicio incluido!";
"species.about.cat_green" = "¡Felino ecológico! Se vuelve verde mientras hace la rutina clásica de gato: comer, dormir, repetir!";
"species.about.cat_grumpy" = "¡Perpetuamente no impresionado! La mirada mortal de este gatito podría detener el tráfico... ¡y otros gatos!";
"species.about.cat_house" = "¡Hotel felino de cinco estrellas! El santuario de siesta perfecto para que tus gatos digitales descansen sus patas!";
"species.about.cat_pinky" = "¡Vibras de pantera rosa sin copyright! Hace poses y roba corazones!";
"species.about.cat_strawberry" = "¡Dulce como una fresa, sassy como un gato! El compañero perfecto de postre para tu escritorio!";
"species.about.cat_white" = "¡Pura elegancia en movimiento! Hábitos alimenticios prístinos y posiciones de sueño angelicales!";
"species.about.cayman718" = "¡El único Porsche dentro del presupuesto! Señala estilo y acelera tus sueños de escritorio! 💸";
"species.about.clockanalog" = "Simplemente muestra la hora actual y no hace nada más... 😅";
"species.about.clockdigital" = "Simplemente muestra la hora actual y no hace nada más... 😅";
"species.about.cromulon" = "¡MUÉSTRAME LO QUE TIENES! Energía de cabeza gigante con juicio cósmico y actitud interdimensional!";
"species.about.cromulon_pink" = "¡Personalidad rosa del tamaño de un planeta! Sigue juzgando tus elecciones de escritorio con estilo fabuloso!";
"species.about.cromulon_rainbow" = "Una especie de seres del tamaño de un planeta con forma de cabezas humanas gigantes... \n '¡Muéstrame lo que tienes!'";
"species.about.crow" = "¡Brillantez negra voladora! Vuela por ahí, esparce amor y demuestra que la inteligencia viene en paquetes oscuros!";
"species.about.crow_white" = "¡Inteligencia albina rara! Alas blancas de sabiduría que traen vibras de invierno a tu espacio de trabajo!";
"species.about.cybertruck" = "🔲⚡💪🛠️🪟🔒💥 ¡Perfección poligonal! Se carga, señala giros y ocasionalmente rompe expectativas!";
"species.about.frog" = "¡Magia anfibia hipnótica! Parpadea, come moscas y te pone bajo un hechizo lindo!";
"species.about.frog_venom" = "¡El peligro viene en paquetes pequeños y adorables! Hermoso pero mortal: ¡admira desde una distancia segura!";
"species.about.gazebo" = "Un poco de sombra en el jardín, para dormir en la hierba...";
"species.about.german" = "¡Perro de trabajo serio con corazón suave! Protector profesional, adicto amateur al sofá!";
"species.about.hedgehog" = "¡Espinoso por fuera, dulce por dentro! Se enrolla en una pelota perfecta cuando la vida se vuelve abrumadora!";
"species.about.jeansbear" = "¡Sueños de mezclilla y esquemas de oso de peluche! Vibras de viernes casual en forma permanente de oso!";
"species.about.koala" = "¡Acróbata con poder de eucalipto! Hace volteretas por amor y demuestra que 'funcionalmente extinto' es solo un estado mental!";
"species.about.koala_pirate" = "¡Ahoy marinero! Este marsupial navegante demuestra que la vida pirata lo eligió a él, no al revés!";
"species.about.labrador" = "¡El amor de América con cuatro patas! Leal, amoroso y siempre listo para paseos!";
"species.about.milo" = "¡Sobrecarga de energía de buen chico! Persigue sueños, mastica huesos y duerme como el mejor perrito!";
"species.about.moth" = "Esas alas eran un dolor de cabeza, déjame decirte eso..!";
"species.about.mushroom" = "¡Perfecto para risotto, perfecto para escritorio! Este hongo trae vibras de bosque a tu pantalla!";
"species.about.mushroom_amanita" = "¡Bonito pero venenoso! Todo el encanto del bosque con un lado de 'no tocar'!";
"species.about.mushroomwizard" = "¡Maestro mágico relajado! Duerme a través de hechizos y demuestra que la relajación es el poder definitivo!";
"species.about.nyan" = "Un homenaje a uno de los gatos más famosos de Internet.";
"species.about.panda" = "¡Maestro zen de los bocadillos! Medita, hace volteretas, maneja sables de luz y toma selfies: ¡perfección panda suprema!";
"species.about.panda_vest" = "No se cobra por el mejor... o el más guapo";
"species.about.pikachu" = "⚡ ¡Perfección de monstruo de bolsillo! Personalidad eléctrica con encanto legendario: ¡hay que atrapar este sentimiento!";
"species.about.poop" = "¡Apesta tu pantalla con estilo! Huele, habla basura y deja manchas cuestionables: ¡humor de baño supremo!";
"species.about.sheep" = "¡La inspiración OG de mascotas de escritorio! Salta por ahí, mastica hierba y ocasionalmente tiene momentos existenciales!";
"species.about.sheep_black" = "¡El rebelde del rebaño! La misma energía de oveja, pero con esa estética oscura misteriosa!";
"species.about.shiba" = "¡Much wow! ¡Such desktop! ¡Very meme! El buen chico favorito de internet hace una pose!";
"species.about.sloth" = "¡Vida en el carril lento! Toma selfies a 0.5x velocidad y maneja sables de luz con paciencia zen!";
"species.about.sloth_swag" = "¡Lento y elegante! Demuestra que la velocidad está sobrevalorada cuando tienes tanto estilo!";
"species.about.snail" = "¡Molusco que maúlla! El primo de Gary trae vibras de Bob Esponja a paso de caracol!";
"species.about.snail_nicky" = "¡Perfección rosa personalizada! Ternura en cámara lenta especialmente solicitada para máximo factor 'd'aww'!";
"species.about.sunflower" = "Tal y como solicitó @spritz97";
"species.about.trex" = "¡Brazos pequeños, personalidad ENORME! Ruge, toca guitarras, escupe fuego y toma selfies como una rockstar prehistórica!";
"species.about.trex_blue" = "¡Ceratosaurus genial! Todo el swagger del T-Rex con acción de cuerno bonus y encanto de dinosaurio!";
"species.about.trex_violet" = "¡Poder púrpura prehistórico! Vibras de los Picapiedra se encuentran con caos moderno en estilo jurásico!";
"species.about.trex_yellow" = "¡Era dorada de los dinosaurios! Brilla como un diamante prehistórico con actitud de reptil real!";
"species.about.ufo" = "¡Visitante intergaláctico! Se estrella, crea portales y bombardea tu escritorio con magnificencia alienígena!";
"species.about.klee" = "¡Personalidad explosiva! Esta caballero chispa trae bombas, saltos y magia de Genshin a tu escritorio!";
"species.about.raiden" = "¡Elegancia de Arconte Electro! Cocina tormentas, ataca con estilo y gobierna con gracia divina!";
"species.about.keqing" = "¡Líder de Liyue rápida como un rayo! Cambia posturas, ataca con precisión y maneja negocios como una jefa!";
